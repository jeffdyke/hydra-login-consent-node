{
  "name": "hydra-login-consent-logout",
  "version": "2.4.0-pre.3",
  "bin": {
    "hydra-login-consent-logout": "lib/app.js"
  },
  "type": "module",
  "main": "lib/app.js",
  "files": [
    "lib",
    "views"
  ],
  "scripts": {
    "_tsc": "tsc -p .",
    "_bundle": "rollup -c",
    "build": "npm-run-all clean _tsc _bundle",
    "clean": "rm -rf lib dist",
    "serve": "node dist/app.js",
    "serve:dev": "node --enable-source-maps --loader ts-node/esm --env-file=/etc/bondlink/hydra/.env --env-file=/etc/bondlink/hydra/.env.auth.hydra dist/app.js",
    "start": "ts-node-dev --enable-source-maps --loader ts-node/esm --project tsconfig.json --env-file=/etc/bondlink/hydra/.env --env-file=/etc/bondlink/hydra/.env.auth.hydra --env-file=/etc/bondlink/hydra/.env.auth.google --watch public,views --respawn dist/app.js",
    "test": "npm-run-all build",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "tsc"
  },
  "prettier": "ory-prettier-styles",
  "dependencies": {
    "@ory/client": "^1.22.7",
    "@ory/client-fetch": "^1.22.6",
    "@ory/hydra-client": "^2.4.0-alpha.1",
    "@tsed/di": "^8.18.0",
    "@tsed/json-mapper": "^8.17.3",
    "@tsed/logger": "^8.0.4",
    "@tsed/schema": "^8.17.3",
    "@typed/either": "^8.3.0",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cookie-parser": "^1.4.9",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.7.1",
    "@types/url-join": "^4.0.0",
    "axios": "^1.12.2",
    "body-parser": "^2.2.0",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "^1.4.7",
    "csrf-csrf": "^4.0.3",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "google-auth-library": "^10.4.0",
    "pug": "^3.0.3",
    "rotating-file-stream": "^3.2.7",
    "serve-favicon": "^2.5.1",
    "tslog": "^4.10.2",
    "typescript": "^5.9.3",
    "url-join": "^5.0.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@ory/hydra-client-fetch": "^2.4.0-alpha.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.2",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/express": "^5.0.3",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^24.7.1",
    "@types/serve-favicon": "^2.5.7",
    "npm-run-all": "^1.8.0",
    "ory-prettier-styles": "^1.3.0",
    "rollup": "^4.52.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.2.3",
    "ts-node-dev": "^2.0.0"
  }
}
