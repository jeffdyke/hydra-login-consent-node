{
  "name": "hydra-login-consent-logout",
  "version": "2.4.0-pre.3",
  "bin": {
    "hydra-login-consent-logout": "lib/app.js"
  },
  "type": "module",
  "main": "lib/app.js",
  "files": [
    "lib",
    "views"
  ],
  "scripts": {
    "_tsc": "tsc -p .",
    "tswatch": "tsc --watch -p .",
    "_bundle": "rollup -c",
    "build": "npm-run-all clean _tsc _bundle",
    "clean": "rm -rf lib dist",
    "serve": "node --env-file=/etc/hydra-headless-ts/staging.env --env-file=/etc/hydra-headless-ts/.env.auth.hydra --env-file=/etc/hydra-headless-ts/.env.auth.google dist/app-fp.js ",
    "serve:local": "node --enable-source-maps --loader ts-node/esm --env-file=./src/env/local.env dist/app-fp.js",
    "serve:staging": "node --enable-source-maps --loader ts-node/esm --env-file=./src/env/staging.env dist/app-fp.js",
    "serve:production": "node --enable-source-maps --loader ts-node/esm --env-file=./src/env/production.env dist/app-fp.js",
    "serve:dev": "node --enable-source-maps --loader ts-node/esm --env-file=./src/env/local.env dist/app.js",
    "start": "ts-node-dev --enable-source-maps --loader ts-node/esm --project tsconfig.json --env-file=/etc/hydra-headless-ts/local.env --watch public,views --respawn dist/app-fp.js",
    "start:local": "ts-node-dev --enable-source-maps --loader ts-node/esm --project tsconfig.json --env-file=/etc/hydra-headless-ts/local.env --watch public,views --respawn dist/app-fp.js",
    "start:staging": "ts-node-dev --enable-source-maps --loader ts-node/esm --project tsconfig.json --env-file=/etc/hydra-headless-ts/staging.env --env-file=/etc/hydra-headless-ts/.env.auth.hydra --env-file=/etc/hydra-headless-ts/.env.auth.google --respawn dist/app-fp.js",
    "start:production": "ts-node-dev --enable-source-maps --loader ts-node/esm --project tsconfig.json --env-file=./src/env/production.env --watch public,views --respawn dist/app-fp.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
    "typecheck": "tsc --noEmit",
    "validate": "npm-run-all typecheck lint test",
    "prepublishOnly": "tsc"
  },
  "prettier": "ory-prettier-styles",
  "dependencies": {
    "@effect/platform": "^0.92.1",
    "@effect/schema": "^0.75.5",
    "@ory/client-fetch": "^1.22.6",
    "@tsed/di": "^8.18.0",
    "@tsed/json-mapper": "^8.17.3",
    "@tsed/logger": "^8.0.4",
    "@typed/either": "^8.3.0",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cookie-parser": "^1.4.9",
    "@types/url-join": "^4.0.0",
    "axios": "^1.12.2",
    "body-parser": "^2.2.0",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "^1.4.7",
    "csrf-csrf": "^4.0.3",
    "dotenv": "^17.2.3",
    "effect": "^3.18.4",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "google-auth-library": "^10.4.0",
    "http-proxy-middleware": "^3.0.5",
    "ioredis": "^5.8.2",
    "pug": "^3.0.3",
    "redis": "^5.9.0",
    "rotating-file-stream": "^3.2.7",
    "serve-favicon": "^2.5.1",
    "tslog": "^4.10.2",
    "typescript": "^5.9.3",
    "url-join": "^5.0.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@effect/vitest": "^0.26.0",
    "@ory/hydra-client-fetch": "^2.4.0-alpha.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.2",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/express": "^5.0.3",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^24.10.0",
    "@types/serve-favicon": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "c8": "^10.1.3",
    "eslint": "^9.39.1",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-promise": "^7.2.1",
    "npm-run-all": "^1.8.0",
    "ory-prettier-styles": "^1.3.0",
    "prettier": "^3.6.2",
    "rollup": "^4.52.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.2.3",
    "ts-node-dev": "^2.0.0",
    "vitest": "^3.2.4"
  }
}
