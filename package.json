{
  "name": "hydra-login-consent-logout",
  "version": "2.4.0-pre.3",
  "bin": {
    "hydra-login-consent-logout": "lib/app.js"
  },
  "type": "module",
  "main": "lib/app.js",
  "files": [
    "lib",
    "views"
  ],
  "scripts": {
    "_tsc": "tsc -p .",
    "tswatch": "tsc --watch -p .",
    "_bundle": "rollup -c",
    "build": "npm-run-all clean _tsc _bundle",
    "ci": "npm-run-all clean lint _tsc test",
    "clean": "rm -rf lib dist",
    "cli": "node --enable-source-maps --loader ts-node/esm --env-file=./src/env/local.env src/cli.ts",
    "cli:staging": "node --enable-source-maps --loader ts-node/esm --env-file=./src/env/staging.env src/cli.ts",
    "cli:production": "node --enable-source-maps --loader ts-node/esm --env-file=./src/env/production.env src/cli.ts",
    "validate-token": "node --enable-source-maps --loader ts-node/esm --env-file=/etc/hydra-headless-ts/local.env src/cli-validate-token.ts",
    "validate-token:staging": "node --enable-source-maps --loader ts-node/esm --env-file=/etc/hydra-headless-ts/staging.env src/cli-validate-token.ts",
    "validate-token:production": "node --enable-source-maps --loader ts-node/esm --env-file=/etc/hydra-headless-ts/production.env src/cli-validate-token.ts",
    "serve": "node --env-file=/etc/hydra-headless-ts/staging.env dist/app-fp.js",
    "serve:local": "NODE_ENV='--env-file=/etc/hydra-headless-ts/hydra.env' node --enable-source-maps --loader ts-node/esm dist/app-fp.js",
    "serve:staging": "NODE_ENV='--env-file=/etc/hydra-headless-ts/hydra.env' ts-node-dev --enable-source-maps --loader ts-node/esm --project tsconfig.json --respawn dist/app-fp.js",
    "serve:prod": "NODE_ENV='--env-file=/etc/hydra-headless-ts/hydra.env' ts-node-dev --enable-source-maps --loader ts-node/esm --project tsconfig.json --respawn dist/app-fp.js",
    "serve:production": "NODE_ENV='--env-file=/etc/hydra-headless-ts/hydra.env' ts-node-dev --enable-source-maps --loader ts-node/esm --project tsconfig.json --respawn dist/app-fp.js",
    "serve:dev": "node --enable-source-maps --loader ts-node/esm --env-file=/etc/hydra-headless-ts/local.env dist/app-fp.js",
    "start": "ts-node-dev --enable-source-maps --loader ts-node/esm --project tsconfig.json --env-file=/etc/hydra-headless-ts/local.env --watch public,views --respawn dist/app-fp.js",
    "start:local": "NODE_ENV='--env-file=/etc/hydra-headless-ts/local.env' ts-node-dev --enable-source-maps --loader ts-node/esm --project tsconfig.json --watch public,views --respawn dist/app-fp.js",
    "start:staging": "NODE_ENV='--env-file=/etc/hydra-headless-ts/hydra.env' ts-node-dev --enable-source-maps --loader ts-node/esm --project tsconfig.json --respawn dist/app-fp.js",
    "start:production": "NODE_ENV='--env-file=/etc/hydra-headless-ts/hydra.env' ts-node-dev --enable-source-maps --loader ts-node/esm --project tsconfig.json --watch public,views --respawn dist/app-fp.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
    "typecheck": "tsc --noEmit",
    "validate": "npm-run-all typecheck lint test",
    "prepublishOnly": "tsc"
  },
  "prettier": "ory-prettier-styles",
  "dependencies": {
    "@effect/cli": "^0.72.0",
    "@effect/platform-node": "^0.100.0",
    "@kitajs/html": "^4.2.10",
    "@ory/client-fetch": "^1.22.6",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cookie-parser": "^1.4.9",
    "axios": "^1.12.2",
    "body-parser": "^2.2.0",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "^1.4.7",
    "csrf-csrf": "^4.0.3",
    "dotenv": "^17.2.3",
    "effect": "^3.18.4",
    "effect-log": "^0.36.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "google-auth-library": "^10.4.0",
    "http-proxy-middleware": "^3.0.5",
    "ioredis": "^5.8.2",
    "jose": "^6.1.2",
    "rotating-file-stream": "^3.2.7",
    "serve-favicon": "^2.5.1",
    "tslib": "^2.8.1",
    "tslog": "^4.10.2",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@kitajs/ts-html-plugin": "^4.1.3",
    "@ory/hydra-client-fetch": "^2.4.0-alpha.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.2",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/express": "^5.0.3",
    "@types/node": "^24.10.0",
    "@types/serve-favicon": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.1",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-promise": "^7.2.1",
    "npm-run-all": "^4.1.5",
    "ory-prettier-styles": "^1.3.0",
    "prettier": "^3.6.2",
    "rollup": "^4.52.4",
    "rollup-plugin-copy2": "^0.4.0",
    "ts-node-dev": "^2.0.0",
    "vitest": "^3.2.4"
  }
}
