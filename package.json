    "@tsed/json-mapper": "^8.17.3",
    "@tsed/logger": "^8.0.4",
    "@tsed/logger-file": "^8.0.4",
    "@tsed/logger-pattern-layout": "^8.0.4",
    "@tsed/logger-std": "^8.0.4",
    "@tsed/platform-express": "^8.18.0",
    "@tsed/schema": "^8.17.3",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cookie-parser": "^1.4.9",
    "@types/json-schema": "^7.0.15",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.7.1",
    "@types/url-join": "^4.0.0",
    "axios": "^1.12.2",
    "body-parser": "^2.2.0",
    "client-fetch-local": "file:./client-fetch-local",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "^1.4.7",
    "csurf": "^1.11.0",
    "debug": "^4.4.3",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "google-auth-library": "^10.4.0",
    "googleapis": "^162.0.0",
