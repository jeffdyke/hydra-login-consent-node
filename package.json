    "pg-pool": "^3.10.1",
    "pug": "^3.0.3",
    "query-string": "^9.3.1",
    "querystring": "^0.2.1",
    "rotating-file-stream": "^3.2.7",
    "serve-favicon": "^2.5.1",
    "source-map-support": "^0.5.21",
    "tslog": "^4.10.2",
    "typescript": "^5.9.3",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@ory/hydra-client-fetch": "^2.4.0-alpha.1",
    "@rollup/plugin-babel": "^6.0.4",
